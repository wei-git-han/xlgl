<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.app.xlgl.dao.XlglCarsManagerDao">

	<select id="queryObject" resultType="com.css.app.xlgl.entity.XlglCarsManager">
		select * from XLGL_CARS_MANAGER where ID = #{value}
	</select>

	<select id="queryList" resultType="com.css.app.xlgl.entity.XlglCarsManager">
		select * from XLGL_CARS_MANAGER 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from XLGL_CARS_MANAGER 
	</select>
	 
	<insert id="save" parameterType="com.css.app.xlgl.entity.XlglCarsManager">
		insert into XLGL_CARS_MANAGER
		(
			"ID", 
			"CREATED_TIME",
			"UPLOAD_USER",
			"FILE_NAME",
			"INFO_ID",
			"FILE_SERVER_FORMAT_ID",
			"TYPE"
		)
		values
		(
			#{id}, 
			#{createdTime},
			#{uploadUser},
			#{fileName},
			#{infoId},
			#{fileServerFormatId},
			#{type}
		)
	</insert>
	 
	<update id="update" parameterType="com.css.app.xlgl.entity.XlglCarsManager">
		update XLGL_CARS_MANAGER 
		<set>
			<if test="createdTime != null">"CREATED_TIME" = #{createdTime}, </if>
			<if test="uploadUser != null">"UPLOAD_USER" = #{uploadUser}, </if>
			<if test="fileName != null">"FILE_NAME" = #{fileName}, </if>
			<if test="infoId != null">"INFO_ID" = #{infoId}, </if>
			<if test="fileServerFormatId != null">"FILE_SERVER_FORMAT_ID" = #{fileServerFormatId}</if>
			<if test="type != null">"TYPE" = #{type}</if>
		</set>
		where ID = #{id}
	</update>
	
	<delete id="delete">
		delete from XLGL_CARS_MANAGER where ID = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from XLGL_CARS_MANAGER where ID in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>