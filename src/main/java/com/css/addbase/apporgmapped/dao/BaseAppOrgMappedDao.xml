<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.addbase.apporgmapped.dao.BaseAppOrgMappedDao">

	<select id="queryList" resultType="com.css.addbase.apporgmapped.entity.BaseAppOrgMapped">
		select * from BASE_APP_ORG_MAPPED where 1=1
		<if test="appId != null and appId != ''">
			and APP_ID = #{appId}
		</if>
		<if test="type != null and type != ''">
			and TYPE = #{type}
		</if>
		<if test="orgId != null and orgId != ''">
			<!-- and ORG_ID = #{orgId} -->
		</if>
		<if test="appLevel != null and appLevel != ''">
			and APP_LEVEL = #{appLevel}
		</if>
		<if test="ministryApp != null and ministryApp != ''">
			and MINISTRY_APP = #{ministryApp}
		</if>
	</select>
	
</mapper>